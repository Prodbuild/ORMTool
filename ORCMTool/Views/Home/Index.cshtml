@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="~/Contents/css/home.css" rel="stylesheet" />
</head>
<body>
    <div class="container">

        <div class="header">
            <a href="/" class="logo" title="TriZetto, A Cognizant Company">Healthcare IT Solutions &amp; Services</a>
        </div>
        <div class="subheader">
            <div class="applicationtext"><label>oRCM Application</label></div>
            <div class="patientSearchContainer"><input type="text" placeholder="SEARCH A PATIENT" /></div>
            <div class="advancedSearchContainer"><input type="button" value="ADVANCED SEARCH" /></div>
        </div>
        <div class="headerbreak"></div>
        <div style="background-color:blue;height:100%;">
            <table class="workingcontainer nopadding">
                <tr>
                    <td style="width:20%;background-color:#F1F2F7;" class="nopadding">
                        <div class="divMenu" style="background-color:#ffffff;">
                            <div class="red-indicator">

                            </div>
                            <div class="home-container">
                                &nbsp;
                            </div>
                            <div class="selecteddivContainerText">
                                <div>HOME</div>
                            </div>
                        </div>
                        <div class="divMenu">
                            <div class="indicator">

                            </div>
                            <div class="downloadapplication-container">
                                &nbsp;
                            </div>
                            <div class="divContainerText">
                                <div>DOWNLOAD APPLICATION</div>
                            </div>
                        </div>
                        <div class="divMenu">
                            <div class="indicator">

                            </div>
                            <div class="componentteam-container">
                                &nbsp;
                            </div>
                            <div class="divContainerText">
                                <div>COMPONENT TEAM</div>
                            </div>
                        </div>
                        <div class="divMenu">
                            <div class="indicator">

                            </div>
                            <div class="query-container">
                                &nbsp;
                            </div>
                            <div class="divContainerText">
                                <div>QUERY</div>
                            </div>
                        </div>
                        <div class="divMenu">
                            <div class="indicator">

                            </div>
                            <div class="myTasks-container">
                                &nbsp;
                            </div>
                            <div class="divContainerText">
                                <div>MY TASKS</div>
                            </div>
                        </div>
                        <div class="divMenu">
                            <div class="indicator">

                            </div>
                            <div class="request-container">
                                &nbsp;
                            </div>
                            <div class="divContainerText">
                                <div>MY REQUESTS</div>
                            </div>
                        </div>

                    </td>
                    <td style="vertical-align:top;margin:0px 5px 0px 0px;">
                        <div class="screenTextcontainer nopadding"><h2>Agents Screens</h2></div>
                        <div class="workItemContainer">
                            <div class="splitter">&nbsp;</div>
                            <div class="myWorkItems">
                                <div class="myWorkItemsIndicator"></div>
                                <div class="myWorkItemsNumber">35</div>
                                <div class="myworkitemtext">My Work<br />Items</div>
                            </div>
                            <div class="splitter">&nbsp;</div>
                            <div class="myWorkItems">
                                <div class="unselectedWorkItemIndicator"></div>
                                <div class="worklogNumber">25</div>
                                <div class="myworkitemtext">Work<br />Log</div>
                            </div>
                            <div class="splitter">&nbsp;</div>
                            <div class="myWorkItemslong">
                                <div class="unselectedWorkItemIndicator"></div>
                                <div class="reworkqueueNumber">145</div>
                                <div class="myworkitemtext" style="margin:4px 0px 0px 20px">Rework<br />Queue</div>
                            </div>
                            <div class="splitter">&nbsp;</div>
                            <div class="myWorkItemslong">
                                <div class="unselectedWorkItemIndicator"></div>
                                <div class="openqueueNumber">105</div>
                                <div class="myworkitemtext" style="margin:4px 0px 0px 20px">Open<br />Queue</div>
                            </div>
                            <div class="splitter">&nbsp;</div>
                            <div class="myWorkItemslong">
                                <div class="unselectedWorkItemIndicator"></div>
                                <div class="myRequestNumber">25</div>
                                <div class="myworkitemtext">My<br />Requests</div>
                            </div>
                            <div class="splitter">&nbsp;</div>
                            <div class="myWorkItemslong">
                                <div class="unselectedWorkItemIndicator"></div>
                                <div class="myNotificationNumber">15</div>
                                <div class="myworkitemtext">My<br />Notifications</div>
                            </div>
                        </div>

                    </td>
                </tr>
            </table>
        </div>
    </div>
    <div class="footer">&copy;2015 Cognizant, all rights reserved</div>
</body>
</html>
